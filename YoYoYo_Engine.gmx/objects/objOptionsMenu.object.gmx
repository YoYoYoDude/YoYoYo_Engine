        <arguments>
          <argument>
            <kind>1</kind>
            <string>//animate player sprite
playerIndex = (playerIndex + 1/5) mod 4;

if (keyboard_check_pressed(vk_up))    //global.upbutton
{
    audio_play_sound(global.menuSound,0,0);
    select -= 1;
    if(select &lt; 1)
        select = 5;
}
else if (keyboard_check_pressed(vk_down))   //global.downbutton
{
    audio_play_sound(global.menuSound,0,0);
    select += 1;
    if(select &gt; 5)
        select = 1;
}
else if (keyboard_check_pressed(ord("Z"))) //global.shotbutton
{
    //save changes and go back
    scrSaveConfig();
    room_goto(rTitle);
    exit;
}
else if (keyboard_check_pressed(vk_shift)) //global.jumpbutton
{
    if (select == 1)    //toggle mute music
    {
        scrToggleMusic();
    }
    else if (select == 3)   //toggle fullscreen
    {
        global.fullscreenMode = !global.fullscreenMode;
        
        scrSetFullscreen();
    }
    else if (select == 4)   //toggle smoothing mode
    {
        global.smoothingMode = !global.smoothingMode;
    }
    else if (select == 5)   //go to the controls menu
    {
        scrSaveConfig();    //save changes
        instance_create(x,y,objControlsMenu);
        instance_destroy();
        exit;
    }
}

if (keyboard_check(vk_right))  //global.rightbutton
{
    if (select == 2)    //raise volume
    {
        if (global.volumeLevel &lt; 100)
                global.volumeLevel += 1;
        
        audio_master_gain(global.volumeLevel/100);  //set master gain
    }
}
else if (keyboard_check(vk_left))    //global.leftbutton
{
    if (select == 2)    //lower volume
    {
        if (global.volumeLevel &gt; 0)
            global.volumeLevel -= 1;
        
        audio_master_gain(global.volumeLevel/100);  //set master gain
    }
}
</string>
          </argument>
        </arguments>
      </action>
    </event>
